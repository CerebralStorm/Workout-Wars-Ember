<ul class="list-group">
  {{#each controller}}
    <li class="list-group-item">
      <div class="row">    
        <br>
        <div class="col-md-2 col-sm-3 text-center">
          <a class="story-img" href="#"><img src="/assets/dumbbell-icon.png" style="width:60px;height:60px" class="img-circle"></a>
        </div>
        <div class="col-md-10 col-sm-9">
          <h5>{{description}}</h5>
          <div class="row">
            <div class="col-xs-9">
              <ul class="list-inline"><li><a href="#">{{shortDateTime createdAt}}</a></li><li><a href="#"></ul>
              </div>
            <div class="col-xs-3"></div>
          </div>
          <br><br>
        </div>
      </div>
      <hr>
    </li>
  {{/each}}
</ul>

        

