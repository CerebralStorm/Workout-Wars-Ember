<div class="panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title">Exercise Details</h3>
  </div>
  <div class="panel-body">        
    <a href="#" class="btn btn-danger pull-right" {{action "delete" this}}>Delete</a>
    
    <h2>{{name}}</h2>
    <p><strong>Description:</strong> {{description}}</p>
    <p><strong>Measurement:</strong> {{metric.name}}</p>
    <p><strong>Experience Multiplier:</strong> {{experienceMultiplier}}</p>
    <p><strong>Created At:</strong> {{date createdAt}}</p>
  </div>
</div>