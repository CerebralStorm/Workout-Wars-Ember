<div class="row">
  <div class="col-md-3">
    <hr>
    {{#link-to "competitions.create" class="btn btn-success btn-block"}}New Competition{{/link-to}}
    <hr>
    <h4>Active Competitions</h4>
    <ul class="nav">
    {{#each controller}}
      {{#unless isDirty}}
      <li>
        {{#link-to "competition" this}}{{name}}{{/link-to}}
      </li>
      {{/unless}}
    {{else}}
      <li>There are no competitions.</li>
    {{/each}}
    </ul>
  </div>

  <div class="col-md-9">
    {{outlet}}
  </div>
</div>


