<div class="container">
  <div class="jumbotron">
    <img src="/assets/ww-logo-large.png" class="img-responsive" />    
    <h1><p class="text-danger text-center">BETA</p></h1>
    <p>Get ready to take your fitness to the next level.</p>
  </div>

  <hr class="featurette-divider">

  <h2 class="featurette-heading">Current User Levels <span class="text-muted">Join the madness</span></h2>

  <div class="panel panel-danger">
    <table class="table table-hover">
      <thead>
        <tr class="active">
          <th>User</th>
          <th>Level</th>
          <th>Experience</th>
        </tr>
      </thead>
      <tbody>
      {{#each user in controller}}
      <tr>
        <td>{{user.handle}}</td>
        <td><span class="badge">{{user.level}}</span></td>               
        <td><span class="badge">{{user.experience}}</span></td>
      </tr>
      {{/each}}
      </tbody>
    </table>
  </div>


  <hr class="featurette-divider">
</div>