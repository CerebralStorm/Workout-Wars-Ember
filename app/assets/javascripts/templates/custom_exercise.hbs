<div class="panel panel-info">
  <div class="panel-heading">
    <h3 class="panel-title">Custom Exercise Details</h3>
  </div>
  <div class="panel-body">        
    <a href="#" class="btn btn-danger pull-right" {{action "delete" this}}>Delete</a>
    <h2>{{this.exercise.name}}</h2>
    <p>{{this.exercise.description}}</p>
    <p>{{this.exercise.experienceMultiplier}}</p>
    <p>Created At: {{date this.exercise.createdAt}}</p>
  </div>
</div>