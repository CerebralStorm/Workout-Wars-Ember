<h1>Team Competitions</h1>
{{#linkTo "teamCompetitions.create" class="btn btn-lg btn-success btn-block"}}New Team Competition{{/linkTo}}
<hr>
<div class="row">
  <div class="col-md-3">
    <ul class="nav nav-pills nav-stacked">
    {{#each controller}}
      <li>{{#linkTo "teamCompetition" this}}{{name}}{{/linkTo}}</li>
    {{else}}
      <li>There are no team competitions.</li>
    {{/each}}
    </ul>
  </div>
  <div class="col-md-9">
    {{outlet}}
  </div>
</div>
