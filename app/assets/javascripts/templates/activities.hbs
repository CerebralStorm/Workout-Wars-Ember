<div class="row">
  <div class="col-md-4">
    <div class="panel panel-primary">
      <div class="panel-heading">
        <h3 class="panel-title">Logged Activities</h3>
      </div>
      <ul class="list-group">
        {{#each controller}}
          <li class="list-group-item">{{#link-to "activity" this}}{{date this.createdAt}} - {{this.exercise.name}}{{/link-to}}</li>
        {{/each}}
      </ul>
    </div>
  </div>

  <div class="col-md-8">
    {{outlet}}
  </div>
</div>
