{{#linkTo 'user.competitions' class="btn btn-info"}}Back to your competitions{{/linkTo}}
<div class="row">
  <div class="col-md-6">
    <h3>{{name}} Ranking</h3>
    <hr>
    <ol>
      {{#each user in users}}
        <li>{{user.email}}
          <ul>
            {{#each user.eventActivities}}
              <li>{{this.activity.exercise.name}} - {{this.activity.reps}} - {{date this.activity.createdAt}}</li>
            {{/each}}
            <hr>
            <li>Total: Coming Soon</li>
          </ul>
        </li>
      {{/each}}
    </ol>
  </div>

  <div class="col-md-6">
    <h3>Exercises for the competition</h3>
    <hr>
    <ul>
      {{#each exercises}}
        <li>{{name}}</li>
      {{/each}}
    </ul>
  </div>
</div>




  
