<h1>Challenges</h1>
{{#linkTo "challenges.create" class="btn btn-lg btn-success btn-block"}}New Challenge{{/linkTo}}
<hr>
<div class="row">
  <div class="col-md-3">
    <ul class="nav nav-pills nav-stacked">
    {{#each controller}}
      <li>{{#linkTo "challenge" this}}{{name}}{{/linkTo}}</li>
    {{else}}
      <li>There are no challenges.</li>
    {{/each}}
    </ul>
  </div>
  <div class="col-md-9">
    {{outlet}}
  </div>
</div>
