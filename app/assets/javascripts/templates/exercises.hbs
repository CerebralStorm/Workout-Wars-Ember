<h1>{{title}}</h1>
<hr>
<div class="row">
  <div class="col-md-6">
    <div class="row">
      <div class="col-md-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <h3 class="panel-title">Existing Exercises</h3>
          </div>
          <div class="list-group">
            {{#each alphabeticalApprovedExercises}}
              {{#unless isDirty}}
                {{#link-to "exercise" this class="list-group-item"}}
                  <span {{bind-attr class="this.status"}}>{{name}}</span>
                {{/link-to}}
              {{/unless}}
            {{else}}
              <a href="#" class="list-group-item">There are no exercises.</a>
            {{/each}}
          </div>
        </div>
      </div>

      <div class="col-md-6">
        <div class="panel panel-info">
          <div class="panel-heading">
            <h3 class="panel-title">Pending Exercises</h3>
          </div>
          <div class="list-group">
            {{#each alphabeticalPendingExercises}}
              {{#unless isDirty}}
                {{#link-to "exercise" this class="list-group-item"}}
                  <span {{bind-attr class="this.status"}}>{{name}}</span>
                {{/link-to}}
              {{/unless}}
            {{else}}
              <a href="#" class="list-group-item">There are no exercises pending approval.</a>
            {{/each}}
          </div>
        </div>
      </div>
    </div>
  </div>

  <div class="col-md-6">
    {{outlet}}
  </div>
</div>
